{
  "name": "inkblot/bind",
  "version": "4.0.1",
  "author": "inkblot",
  "license": "Apache-2.0",
  "summary": "A Puppet module to deploy and manage the ISC BIND server",
  "source": "git://github.com/inkblot/puppet-bind.git",
  "project_page": "https://github.com/inkblot/puppet-bind",
